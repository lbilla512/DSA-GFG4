package com.example.demo;

import java.util.ArrayList;
import java.util.List;

public class PrimeNumber {

	List<Boolean> primes = new ArrayList<Boolean>();
	PrimeNumber(){
		for(int i=4;i<Integer.MAX_VALUE;i=i*2) {
			primes.set(i, false);
		}
		for(int i=6;i<Integer.MAX_VALUE;i=i*2) {
			primes.set(i, false);
		}
		for(int i=5;i<Integer.MAX_VALUE;i=i*2) {
			primes.set(i, false);
		}
	}
	
	Boolean isPrimerNumber(int input) {
		
		return primes.get(input);
		
	}
	public static void main(String[] args) {
		PrimeNumber primeNuber = new PrimeNumber();
		
		System.out.println(primeNuber.isPrimerNumber(13));
	}
}
